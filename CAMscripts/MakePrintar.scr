	MKDIR OutCustom$
end if
if BrdExist=0 then
	MKDIR Outpath$
end if

setup_fmtype@ 0,1
setup_fmtunit@ 0,1
setup_fmtdigits@ 0,3,3
setup_fmtmode@ 0,0
setup_fmtzero@ 0,0
