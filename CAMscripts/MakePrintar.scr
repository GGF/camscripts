MakeDPP:
RotateFlg=0
if OneSide = -1 then
	setlayer@ rcn
else 
	setlayer@ OneSide
end if
layer_alloff@ 1
SizeX#= vismaxx! 
SizeY#= vismaxy! 
'if (SizeX# < 300) AND (SizeY# < 300) then
if (SizeX# < 280) AND (SizeY# < 280) then
	MessageBox  "Блок слишком мал"+CR!+"Ширина блока должна быть не менее 300 мм", "Ошибка",ERROR 
	DPPerror =1
	return
end if
'-------удаление слишком широкой рамки на ДПП ----
layer_alloff@ 0
if WideBorderDPP AND (visminx!<0) AND (visminy!<0) then
for i=1 to 2
	For lay = 0 to Highestlayer! 
		setlayer@ lay
		edit_delete@
		axy@ SizeX#,SizeY#
		back@
	next
	SizeX#= vismaxx! 
	SizeY#= vismaxy! 
	layer_alloff@ 0
next
end if
'----------------- rotate and move 
if SizeX# < SizeY# then 
	layer_alloff@ 0 
	edit_rotate@
	setrot100angle@ 27000
	edit_selectall2@
	axy@ SizeX#/2.0,SizeY#/2.0
	back@
	edit_move@
	edit_selectall2@
	axy@ visminx!,visminy!
	axy@ 0.0,0.0
	back@
	swap SizeX#,SizeY#
	RotateFlg=1
end if


'suspend

TargLeftX# = 5.5
TargBotY# = 12.5.0
TargRightX# = SizeX# - 5.5
TargTopY# = SizeY# - 20.0
